{
  "name": "autonomous-coding-backend",
  "version": "1.0.0",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "prisma generate && tsc",
    "start": "node dist/index.js",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed-test-data.ts",
    "agent:worker": "python services/agent_orchestrator.py worker",
    "agent:server": "uvicorn services.agent_orchestrator:app --reload --port 8000",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "security:env": "tsx -e \"import { runEnvSecurityChecks } from './src/utils/env-security'; runEnvSecurityChecks();\"",
    "security:audit": "npm audit --audit-level=moderate",
    "security:audit:fix": "npm audit fix",
    "security:audit:force": "npm audit fix --force"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@prisma/client": "^5.7.0",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.10",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.0.0",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "socket.io": "^4.7.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.0",
    "@types/sharp": "^0.31.1",
    "@types/supertest": "^6.0.2",
    "@vitest/coverage-v8": "^2.0.0",
    "prisma": "^5.7.0",
    "supertest": "^7.0.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.2",
    "vitest": "^2.0.0"
  }
}
